<!DOCTYPE html>
<html>
  <div id="wrapper">
    <body>
      <%= form_tag admin_students_path, class: "search", method: 'get' do %>
        <%= text_field_tag :search, params[:search] %>
        <%= submit_tag "Search" %>
      <% end %>
      <h3> Students:</h3>
      <div id="generic">
        <% @searched_students.each do |student| %>
          <h4><%= link_to student.f_name, admin_student_path(student) %>
              <%= link_to student.l_name, admin_student_path(student) %>
          </h4>
        <% end %>
      </div>
    <div>
      <%= paginate @students %>
    </div>
    </body>
  </div>
</html>
